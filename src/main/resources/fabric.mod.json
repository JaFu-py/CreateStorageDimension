	"authors": [
		"JaFu.py"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC BY-NC-SA 4.0",
	"icon": "assets/create-storage-dimension/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.jafupy.CreateStorageDimension"
		],"client": [
			"com.jafupy.CreateStorageDimensionClient"
		],
		"fabric-datagen": [
			"com.jafupy.CreateStorageDimensionDataGenerator"
		]
	},
	"mixins": [
		"create-storage-dimension.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
